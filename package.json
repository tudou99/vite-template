{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host --mode development",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "three": "^0.138.3",
    "vant": "^3.4.6",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@amatlash/vite-plugin-stylelint": "^1.2.0",
    "@rollup/plugin-strip": "^2.1.0",
    "@types/node": "^17.0.21",
    "@types/stylelint": "13",
    "@types/three": "^0.138.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-legacy": "^1.6.3",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/runtime-core": "^3.2.26",
    "autoprefixer": "^10.4.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.49.9",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-scss": "3.21.0",
    "typescript": "^4.6.2",
    "unplugin-auto-import": "^0.6.4",
    "unplugin-vue-components": "^0.18.0",
    "vite": "^2.8.6",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-stylelint": "^2.0.1",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.33.1"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --ext *.{ts,js,vue} --rule 'no-console: error'"
    ]
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "three",
        "vant",
        "vant/es",
        "vant/es/button/style/index",
        "vant/es/cell-group/style/index",
        "vant/es/cell/style/index",
        "vant/es/collapse-item/style/index",
        "vant/es/collapse/style/index",
        "vant/es/count-down/style/index",
        "vant/es/dropdown-item/style/index",
        "vant/es/dropdown-menu/style/index",
        "vant/es/notice-bar/style/index",
        "vant/es/popup/style/index",
        "vant/es/search/style/index",
        "vant/es/tab/style/index",
        "vant/es/tabs/style/index",
        "vue",
        "vue-router"
      ]
    }
  }
}
